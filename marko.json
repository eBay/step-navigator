{
    "taglib-id": "se-step-navigator",
    "tags": {
        "se-step-navigator": {
            "template": "./src/index.marko",
            "@title": "string",
            "@previousStepText": "string",
            "@nextStepText": "string",
            "@isProgressStepperHidden": "boolean",
            "@content-styles": "string",
            "width": "string",
            "buttonAlignment": "string",
            "@step <step>[]": {
                "@a11yText": "string",
                "@title": "string",
                "@subtitle": "string",
                "@content": "expression"
            },

            "@static-content": {
                "type": "expression", 
                "target-property": "static"
            }
        }
    }
}